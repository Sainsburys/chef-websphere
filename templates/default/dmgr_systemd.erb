[Unit]
Description=IBM WebSphere Application Server Deployyment Manager Service

[Service]
ExecStart=<%= @profile_path %>/bin/startManager.sh <%= @start_args %>
ExecStop=<%= @profile_path %>/bin/stopManager.sh <%= @stop_args %>
User=<%= @runas_user != '' ? @runas_user : 'root' %>
TimeoutSec=<%= @svc_timeout %>

[Install]
WantedBy=multi-user.target